%!PS-Adobe-2.0 EPSF-2.0
%%BoundingBox: 0 0 420 630
%%Pages: 2
/n0 36 def

/board0
5  0   5  1   5  2   4  2   3  2   2  2   2  3   2  4   2  5
1  5   0  5  -1  5  -1  4  -1  3  -1  2  -2  2  -3  2  -4  2
-4 1  -4  0  -4 -1  -3 -1  -2 -1  -1 -1  -1 -2  -1 -3  -1 -4
0 -4   1 -4   2 -4   2 -3   2 -2   2 -1   3 -1   4 -1   5 -1
n0 2 mul array astore def

/step 30 def
/stone_radius 10 def
/X {step mul} def
/Y {step mul neg} def

/Times-Bold findfont stone_radius scalefont setfont

/circle {
    1 setlinewidth
    newpath exch X exch Y stone_radius 0 360 arc stroke
} def

/fillcircle {
    1 setlinewidth
    newpath exch X exch Y stone_radius 0 360 arc fill stroke
} def

/number {
    exch X exch Y moveto
    4 string cvs show
} def

/line {
    0.5 setlinewidth
    newpath
    exch X exch Y moveto
    exch X exch Y lineto
    stroke
} def

/thickline {
    2 setlinewidth
    newpath
    exch X exch Y moveto
    exch X exch Y lineto
    stroke
} def

/start_game {
    0 1 n0 1 sub {
	2 mul /i exch def
	board0 i get board0 i 1 add get circle
    } for
} def

/up_to_numbered {
    /n exch def
    n 7 mul copy
    start_game
    n { line number } repeat
} def

/up_to {
    /n exch def
    n 7 mul copy
    start_game
    n 0 ne {
	n 1 sub { line circle pop } repeat
	thickline fillcircle pop
    } if
} def

%%Page: -1 -1
180 210 translate
0 up_to_numbered
showpage
%%Page: 0 0
180 210 translate
0 up_to
showpage


